;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"Charts.vue","sourceRoot":"src/components/charts","sourcesContent":["<template>\n  <div class=\"charts-view rounded-lg shadow-lg \">\n    <div class=\"wrap-chart\">\n      <div class=\"title-charts\">\n        <h1>Why do you create a startup?</h1>\n        <p>Trends</p>\n      </div>\n      <div class=\"chart\">\n        <pie-chart :data=\"data\"/>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Charts',\n  data() {\n    return {\n      data: [\n        {\n          name: 'Primero',\n          value: null,\n        },\n        {\n          name: 'Segundo',\n          value: null,\n        },\n        {\n          name: 'Tercero',\n          value: null,\n        },\n      ],\n    }\n  },\n  async mounted () {\n            // Api consumption\n            const response = await fetch('/api/reports');\n            const datos = await response.json();\n            \n            // Objects\n            this.data[0].value = [datos.reports[0].total]\n            this.data[1].value = [datos.reports[1].total]\n            this.data[2].value = [datos.reports[2].total]\n            this.data[0].name = [datos.reports[0].category]\n            this.data[1].name = [datos.reports[1].category]\n            this.data[2].name = [datos.reports[2].category]\n        \n    }\n}\n</script>\n\n<style lang=\"postcss\">\n@tailwind base;\n@tailwind components;\n@tailwind utilities;\n\n.charts-view{\n  width: 100%;\n  height: auto;\n  background-color: white;\n  padding: 20px;\n}\n\n.title-charts h1{\n  color: #666F80;\n  font-size: 25px;\n}\n.title-charts p{\n  color: #ACB8C9;\n  font-size: 21px;\n}\n\n.chart {\n  width: 100%;\n  height: 700px;\n  max-height: 445px;\n}\n\nbody.dark-mode .charts-view{\n  background: #3f3f3f;\n}\n\nbody.dark-mode .title-charts h1,\nbody.dark-mode .title-charts p{\n  color: white;\n}\n\n\n</style>\n"]}]}